SPEC java.security.KeyPair
OBJECTS
	java.security.PrivateKey consPriv;
	java.security.PublicKey consPub;
	
	java.security.PrivateKey retPriv;
	java.security.PublicKey retPub;
	
	KeyPairGenertor generator;
	
EVENTS
	co1 : KeyPair(consPub, consPriv);
	co2: this = generator.generateKeyPair();
	cons := (co1 | co2)

	pu : retPub = getPublic();
	pr : retPriv = getPrivate();
	Gets := pu | pr;

ORDER
	cons, (pu*, pr*)*
	
REQUIRES
	generatedPrivkey[consPriv]; //Different name for predicate? This rule also generates it
	generatedPubkey[consPub]; //Different name for predicate? This rule also generates it
	generatedKeypair[this]
	
ENSURES
	generatedKeypair[this, _] after co; //I think we can remove this here, 
	generatedPubkey[retPub] after pu;
	generatedPrivkey[retPriv] after pr;